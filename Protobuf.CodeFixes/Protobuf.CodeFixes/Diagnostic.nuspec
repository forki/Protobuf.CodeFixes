  <metadata>
    <id>Protobuf.CodeFixes</id>
    <version>1.0.0.0</version>
    <title>Protobuf.CodeFixes</title>
    <authors>Julien Adam</authors>
    <owners>Julien Adam</owners>
    <licenseUrl>https://github.com/julienadam/Protobuf.CodeFixes/blob/master/LICENSE</licenseUrl>
